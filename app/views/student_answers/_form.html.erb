<%= form_for @student_answer do |f| %>
  <%Student.find(session[:student_id]).test.questions.each do |question| %>
    <p><%= question.question_description %></p>
    <% question.options.each do |option|%>
      <%= radio_button("response_item", question.id, option.id)  %><%=option.option_value %><br/>
      <br/>
    <% end %>
  <% end %>
  <%= f.submit%>
<% end %>
