<div class="container">
  <h3> List of Tests</h3>
  <table class="table table-striped">
    <thead>
      <tr>
        <th> Test Name: </th>
        <th colspan="3"> Action </th>
      </tr>
    </thead>
    <tbody>
      <% @tests.each do |test| %>
        <tr>
          <td><%= test.name %></td>
          <td>
            <%= link_to 'Show', test_path(test) %> |
            <%= link_to 'Edit', edit_test_path(test) %> |
            <%= link_to 'Delete', test_path(test), method: :delete, data: {confirm: 'Are You Sure You Want To Delete?'} %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
    <%= link_to 'Add Test', new_test_path, class: "btn btn-primary"%>
    <%= link_to 'Question', questions_path, class: "btn btn-primary" %>
    <%= link_to 'Question Test', questions_tests_path, class: "btn btn-primary" %>
</div>

