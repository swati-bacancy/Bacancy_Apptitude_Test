<h1> Test Index Page</h1>
<%= link_to 'new', new_test_path%>
<table border="1">
  <tr>
    <th> Test Name: </th>
    <th colspan="3"> Action </th>
  </tr>
  <% @tests.each do |test| %>
  <tr>
      <td><%= test.name %></td>
      <td><%= link_to 'show', test_path(test) %></td>
      <td><%= link_to 'edit', edit_test_path(test) %></td>
      <td><%= link_to 'delete', test_path(test), method: :delete, data: {confirm: 'Are You Sure You Want To Delete?'} %></td>
    </tr>
    <% end %>
</table></br>
